{
	"name": "Rekuovers",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "npm start -- --reset-cache",
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"clean:android": "cd android && ./gradlew clean",
		"debug:android": "cd android && ./gradlew installDebug --debug",
		"build:android": "cd android && ./gradlew assembleRelease --info",
		"release:android": "cd android && ./gradlew installRelease",
		"test": "jest",
		"prettier": "prettier --write \"**/*.js\""
	},
	"dependencies": {
		"jwt-decode": "^2.2.0",
		"moment": "^2.20.1",
		"prop-types": "^15.6.0",
		"query-string": "^5.0.1",
		"react": "16.0.0",
		"react-native": "0.52.1",
		"react-native-animatable": "^1.2.4",
		"react-native-banner-carousel": "^1.0.2",
		"react-native-dominant-color": "https://github.com/hu9osaez/react-native-dominant-color.git",
		"react-native-elements": "^0.19.0",
		"react-native-fbsdk": "^0.7.0",
		"react-native-keyboard-aware-scroll-view": "^0.4.3",
		"react-native-swiper": "^1.5.13",
		"react-native-timer": "^1.3.1",
		"react-native-typography": "^1.1.0",
		"react-native-vector-icons": "^4.4.2",
		"react-native-view-more-text": "^1.4.3",
		"react-native-youtube": "https://github.com/inProgress-team/react-native-youtube.git",
		"react-navigation": "^1.1.2",
		"react-redux": "^5.0.6",
		"redux": "^3.7.2",
		"redux-action-buffer": "^1.0.1",
		"redux-logger": "^3.0.6",
		"redux-persist": "^4.3.1",
		"redux-thunk": "^2.2.0",
		"validatorjs": "^3.14.2"
	},
	"devDependencies": {
		"babel-jest": "21.2.0",
		"babel-plugin-module-resolver": "^2.7.1",
		"babel-preset-react-native": "4.0.0",
		"jest": "21.2.1",
		"prettier": "^1.10.2",
		"react-test-renderer": "16.0.0",
		"reactotron-react-native": "^1.14.0",
		"reactotron-redux": "^1.13.0",
		"solidarity": "^1.1.1",
		"solidarity-react-native": "^1.0.0"
	},
	"rnpm": {
		"assets": [
			"./assets/fonts/"
		]
	},
	"jest": {
		"preset": "react-native",
		"testMatch": [
			"**/__tests__/tests/**/*.js"
		],
		"transformIgnorePatterns": [
			"node_modules/(?!(react-native|redux-persist)/)"
		]
	}
}
